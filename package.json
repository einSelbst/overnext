{
  "name": "overnext",
  "version": "1.0.1",
  "description": "Very opinionated next.js typescript boilerplate",
  "keywords": [
    "next.js, cypress",
    "i18n",
    "typescript"
  ],
  "homepage": "https://overnext.vercel.app",
  "bugs": "https://github.com/einSelbst/overnext/issues",
  "author": "einSelbst",
  "private": true,
  "repository": "https://github.com/einSelbst/overnext",
  "scripts": {
    "skip-for-now-preinstall": "npx only-allow pnpm",
    "?dev": "echo Start next app in dev mode",
    "dev": "next",
    "?debug": "echo Start node debugger",
    "debug": "NODE_OPTIONS='--inspect' next",
    "build": "next build",
    "?prebuild:netlify": "echo This shall only run on netlify to install deps via pnpm",
    "prebuild:netlify": "test \"$CI\" = true && npx pnpm i -r --store=node_modules/.pnpm-store || echo skiping pnpm install",
    "build:netlify": "npx pnpm build",
    "postbuild": "next-sitemap",
    "build:ssr": "NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "?serve": "echo Build app and start server",
    "serve": "pnpm build && next start",
    "?servex": "echo Build & run app on public URL",
    "servex": "pnpm serve & npx localtunnel --port 3000",
    "?cz": "Make new commit",
    "cz": "git-cz",
    "?release": "Prepare a new release",
    "release": "standard-version",
    "?docs": "Generate typedoc docs",
    "docs": "typedoc",
    "info": "npm-scripts-info",
    "?find:unused": "echo Find unused files",
    "find:unused": "next-unused",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier-standard --format --lint",
    "lint": "eslint . --fix",
    "?test": "echo Run only some tests for now",
    "test": "jest",
    "?test:jest": "echo Run jest tests",
    "test:jest": "jest",
    "?test:cy": "echo Run cypress tests",
    "test:cy": "cypress open",
    "?e2e": "echo Start test server and run cypress",
    "e2e": "start-test 'pnpm dev' 3000 'pnpm test:cy'",
    "pree2e": "rm -rf .nyc_output coverage .next || true",
    "?validate": "echo Format, lint, type-check & test the app",
    "validate": "pnpm format && pnpm lint && pnpm type-check && pnpm test"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "autoprefixer": "10.2.5",
    "next": "10.0.9-canary.4",
    "next-seo": "4.20.0",
    "next-themes": "0.0.12",
    "postcss": "8.2.7",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tailwindcss": "2.0.3",
    "tslib": "2.1.0"
  },
  "devDependencies": {
    "@axe-core/react": "4.1.1",
    "@babel/core": "7.13.10",
    "@babel/runtime": "7.13.10",
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@cypress/code-coverage": "3.9.2",
    "@next/bundle-analyzer": "10.0.8",
    "@next/eslint-plugin-next": "10.0.8",
    "@testing-library/react": "11.2.5",
    "@types/jest": "26.0.20",
    "@types/node": "13.13.46",
    "@types/prop-types": "15.7.3",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "babel-jest": "26.6.3",
    "commitizen": "4.2.3",
    "cypress": "6.6.0",
    "dependency-version-badge": "1.9.1",
    "eslint": "7.21.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.2.0",
    "eslint-plugin-jest-dom": "3.6.5",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.1.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.4",
    "next-compose-plugins": "2.2.1",
    "next-sitemap": "1.5.16",
    "next-unused": "0.0.3",
    "npm-scripts-info": "0.3.9",
    "prettier-standard": "16.3.0",
    "standard-version": "9.1.1",
    "start-server-and-test": "1.12.0",
    "typedoc": "0.20.30",
    "typescript": "4.2.3"
  },
  "optionalDependencies": {
    "fsevents": "2.3.2"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=5"
  },
  "pnpm": {
    "overrides": {
      "fsevents": "2.3.2",
      "prettierx": "0.17"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "pnpm type-check"
    }
  },
  "lint-staged": {
    "*": [
      "prettier-standard --lint"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "src",
      "styles",
      "test",
      "cypress"
    ],
    "exclude": [],
    "entrypoints": [
      "src"
    ]
  },
  "nyc": {
    "exclude": [
      "src/pages/api/__coverage__.js"
    ]
  }
}
