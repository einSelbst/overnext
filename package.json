{
  "name": "overnext",
  "version": "1.4.0",
  "description": "Very opinionated next.js typescript boilerplate",
  "keywords": [
    "next.js, cypress",
    "i18n",
    "typescript"
  ],
  "homepage": "https://overnext.vercel.app",
  "bugs": "https://github.com/einSelbst/overnext/issues",
  "author": "einSelbst",
  "private": true,
  "repository": "https://github.com/einSelbst/overnext",
  "scripts": {
    "prepare": "is-ci || husky install",
    "preinstall": "npx is-ci || npx only-allow pnpm",
    "postinstall": "is-ci || typesync",
    " ----------------------- Development ----------------------- ": "",
    "?dev": "echo Start next app in dev mode",
    "dev": "pnpm open && next",
    "?debug": "echo Start node debugger",
    "debug": "NODE_OPTIONS='--inspect' next",
    "?build": "echo Build the app",
    "build": "next build && pnpm build:sitemap",
    "build:sitemap": "next-sitemap",
    "?start": "echo Start server to serve the local app build",
    "start": "next start -p 3001",
    "?serve": "echo Build app and start server",
    "serve": "pnpm build && (open http://localhost:3002) && next start -p 3002",
    "serve:3003": "pnpm build && next start -p 3003",
    "?servex": "echo Build & run app on public URL",
    "servex": "pnpm serve & npx localtunnel --port 3000",
    " ------------------------- Testing ------------------------- ": "",
    "?test": "echo Run unit and integration tests once",
    "test": "jest && pnpm e2e",
    "?test:ci": "echo Run jest tests on CI, will re-add cypress later again",
    "test:ci": "jest",
    "?test:jest": "echo Run jest tests and update screenshots",
    "test:jest": "jest -u",
    "?test:cy": "echo Open cypress test runner (needs a running app server)",
    "test:cy": "cypress open",
    "?e2e:watch": "echo Start dev server, open cypress and watch",
    "e2e:watch": "CY_WATCH=true start-test 'next dev -p 3004' 3004 'pnpm test:cy'",
    "?e2e": "echo Build & serve app, then run cypress once",
    "e2e": "CYPRESS_BASE_URL=http://localhost:3003 APP_ENV=test start-test 'pnpm serve:3003' 3003 'cypress run'",
    "e2e2": "CYPRESS_BASE_URL=http://localhost:3003 APP_ENV=test CY_WATCH=false start-test 'pnpm serve:3003' 3003 'cypress run && pnpm checklinks:server'",
    "e2e:cy": "CYPRESS_BASE_URL=http://localhost:3001 APP_ENV=test start-test 'pnpm start' 3001 'cypress run'",
    "pree2e": "rm -rf .nyc_output coverage .next || true",
    " ------------------------- Linting ------------------------- ": "",
    "?format": "echo Format source code and fix issues",
    "format": "prettier --check --write --ignore-unknown . --plugin=prettier-plugin-sh --plugin=prettier-plugin-toml",
    "?lint": "echo Lint source code",
    "lint": "eslint . --cache",
    "?lint:fix": "echo Use deamon to lint source code and fix issues",
    "lint:fix": "eslint_d . --cache --fix --ext .js,.jsx,.ts,.tsx",
    "lint:profile": "TIMING=1 eslint .",
    "?lint:html": "echo Run html-validate on generated static output files",
    "lint:html": "html-validate '.next/**/*.html'",
    "?lint:css": "echo Run stylelint on source code to lint CSS",
    "lint:css": "stylelint 'src/**/*.{css,scss,tsx}' --fix",
    "type-check": "tsc --pretty --noEmit",
    "type-check:watch": "tsc -w --pretty --skipLibCheck --noEmit",
    "?checklinks": "echo Test integrity of hyperlinks in the build files",
    "checklinks": "hyperlink .next/**/*.html --canonicalroot https://overnext.vercel.app/ -pr --root .next/serverless/pages --skip index.html --skip legal --skip account-settings --skip fonts.gstatic --skip chunks --skip ico --skip png --skip svg --skip manifest --skip pages/fauna --skip pages/first --skip pages/dog --skip pages/page --skip _next --todo hybrid --todo ampshared.css --todo ampdoc.css | tap-spot",
    "?checklinks:server": "echo Test integrity of hyperlinks on the local server",
    "checklinks:server": "hyperlink http://localhost:3003/ --canonicalroot https://overnext.vercel.app/ -pr --skip fonts.gstatic --skip api/add-show --skip api/update-show --todo ampshared.css --todo ampdoc.css --todo hybrid | tap-spot",
    "?validate": "echo Validate source code and output files",
    "validate": "pnpm validate:src && pnpm validate:out",
    "?validate:src": "echo Format, lint, type-check & unit test the app",
    "validate:src": "pnpm format && pnpm lint:fix && pnpm lint:css && pnpm type-check && jest",
    "?validate:out": "echo Build app, run integration tests, check links & validate generated html",
    "validate:out": "pnpm e2e2 && pnpm lint:html",
    " ------------------------- Helpers ------------------------- ": "",
    "?build:analyze": "echo Build app and output bundle analyzer diagrams",
    "build:analyze": "ANALYZE=true next build",
    "?build:size": "echo build app and measure loading performance",
    "build:size": "pnpm build && size-limit",
    "open": "open http://localhost:3000",
    "size": "size-limit",
    "?find:unused": "echo Find unused files",
    "find:unused": "next-unused",
    "?build:ssr": "echo used on CI for 'compressed size' action",
    "build:ssr": "NODE_ENV=production BROWSERSLIST_ENV=ssr next build",
    "gh-action:cleanup": "rm package-lock.json",
    "browserslist": "pnpx browserslist@latest --update-db",
    "?cz": "Make new commit",
    "cz": "git-cz",
    "?release": "Prepare a new release",
    "release": "standard-version",
    "?docs": "Generate typedoc docs",
    "docs": "typedoc",
    "?info": "echo Display info about the scripts",
    "info": "npm-scripts-info",
    "eslint-rules:unused": "eslint-find-rules -un .eslintrc.yml --ext .tsx",
    "eslint-rules:current": "eslint-find-rules -cnp .eslintrc.yml",
    "eslint-rules": "eslint-find-rules -dunp .eslintrc.yml --ext .tsx",
    "?stylelint-check": "Check stylelint for rules conflicting prettier",
    "stylelint-check": "stylelint-config-prettier-check",
    "commitlint": "commitlint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/core": "7.16.7",
    "@sentry/nextjs": "6.16.1",
    "@types/node": "14.18.5",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "autoprefixer": "10.4.2",
    "core-js": "3.20.2",
    "faunadb": "4.4.1",
    "is-ci": "3.0.1",
    "next": "11.1.3",
    "next-compose-plugins": "2.2.1",
    "next-pwa": "5.4.4",
    "next-seo": "4.28.1",
    "next-sitemap": "1.6.261",
    "next-themes": "0.0.15",
    "postcss": "8.4.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "1.47.0",
    "tailwindcss": "2.2.19",
    "tslib": "2.3.1",
    "typed-query-selector": "2.6.1",
    "typescript": "4.5.4",
    "webpack": "5.65.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.7",
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@cypress/code-coverage": "3.9.12",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@next/bundle-analyzer": "11.1.3",
    "@next/eslint-plugin-next": "11.1.3",
    "@size-limit/preset-app": "6.0.4",
    "@testing-library/react": "12.1.2",
    "@types/babel__core": "7.1.18",
    "@types/babel__preset-env": "7.9.2",
    "@types/core-js": "2.5.5",
    "@types/eslint": "7.29.0",
    "@types/is-ci": "3.0.0",
    "@types/jest": "27.4.0",
    "@types/prettier": "2.4.2",
    "@types/sass": "1.43.1",
    "@types/source-map-support": "0.5.4",
    "@types/standard-version": "7.0.1",
    "@types/stylelint": "13.13.3",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "@typescript-eslint/typescript-estree": "5.9.0",
    "babel-jest": "27.4.6",
    "babel-plugin-istanbul": "6.1.1",
    "commitizen": "4.2.4",
    "cypress": "9.2.0",
    "cypress-watch-and-reload": "1.5.5",
    "dependency-version-badge": "1.11.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.3",
    "eslint-config-prettier": "8.3.0",
    "eslint-find-rules": "3.6.1",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-array-func": "3.1.7",
    "eslint-plugin-compat": "3.13.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "25.3.4",
    "eslint-plugin-jest-dom": "3.9.4",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-json-schema-validator": "2.1.28",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-promise": "5.2.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-sonarjs": "0.11.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-toml": "0.3.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "eslint-plugin-unicorn": "37.0.1",
    "git-format-staged": "latest",
    "html-validate": "6.1.6",
    "husky": "7.0.4",
    "hyperlink": "4.7.0",
    "jest": "27.4.7",
    "jest-watch-typeahead": "1.0.0",
    "next-unused": "0.0.6",
    "npm-scripts-info": "0.3.9",
    "nyc": "15.1.0",
    "prettier": "2.5.1",
    "prettier-plugin-sh": "0.8.1",
    "prettier-plugin-toml": "latest",
    "size-limit": "6.0.4",
    "source-map-support": "0.5.21",
    "standard-version": "9.3.2",
    "start-server-and-test": "1.14.0",
    "stylelint": "14.2.0",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "23.0.0",
    "stylelint-scss": "4.1.0",
    "tap-spot": "1.1.1",
    "ts-node": "10.4.0",
    "typedoc": "0.22.10",
    "typescript-eslint-language-service": "4.1.5",
    "typesync": "0.8.0"
  },
  "optionalDependencies": {
    "@axe-core/react": "4.3.2",
    "fsevents": "2.3.2",
    "pnpm": "6.25.1"
  },
  "engines": {
    "pnpm": ">=6"
  },
  "pnpm": {
    "overrides": {
      "browserslist": "latest",
      "caniuse-lite": "latest",
      "chokidar": "3",
      "debug": "latest",
      "ansi-regex": "5.0.1",
      "source-map-resolve": "latest",
      "jsonpointer@<5.0.0": ">=5.0.0",
      "json-schema@<0.4.0": ">=0.4.0"
    }
  },
  "next-unused": {
    "alias": {
      "lib/*": [
        "lib/*"
      ],
      "assets/*": [
        "assets/*"
      ],
      "config": [
        "config/"
      ],
      "components/*": [
        "components/*"
      ],
      "layouts/*": [
        "layouts/*"
      ],
      "styles/*": [
        "styles/*"
      ],
      "utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "src"
    ],
    "exclude": [],
    "entrypoints": [
      "src"
    ]
  },
  "volta": {
    "node": "14.18.2",
    "npm": "8.3.0"
  }
}
