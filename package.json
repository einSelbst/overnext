{
  "name": "feehikel",
  "version": "1.0.1",
  "description": "Opinionated next.js boilerplate",
  "keywords": [
    "next.js, cypress"
  ],
  "homepage": "https://feehikel-git-main.einselbst.vercel.app",
  "bugs": "https://github.com/einSelbst/feehikel/issues",
  "author": "einSelbst",
  "private": true,
  "repository": "https://github.com/einSelbst/feehikel",
  "scripts": {
    "skip-for-now-preinstall": "npx only-allow pnpm",
    "?dev": "echo Start next app in dev mode",
    "dev": "next dev",
    "build": "next build",
    "build:ssr": "NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "?cz": "Make new commit",
    "cz": "git-cz",
    "?release": "Prepare a new release",
    "release": "standard-version",
    "?docs": "Generate typedoc docs",
    "docs": "typedoc",
    "info": "npm-scripts-info",
    "?find:unused": "echo Find unused files",
    "find:unused": "next-unused",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier-standard --format --lint",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "?test": "echo Run only some tests for now",
    "test": "jest",
    "?test:jest": "echo Run jest tests",
    "test:jest": "jest",
    "?test:cy": "echo Run cypress tests",
    "test:cy": "cypress open",
    "?e2e": "echo Start test server and run cypress",
    "e2e": "start-test 'pnpm dev' 3000 'pnpm test:cy'",
    "pree2e": "rm -rf .nyc_output coverage .next || true",
    "?validate": "echo Format & lint & type-check & test the app",
    "validate": "pnpm format && pnpm lint && pnpm type-check && pnpm test"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "autoprefixer": "10.2.1",
    "next": "latest",
    "postcss": "8.2.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tailwindcss": "2.0.2"
  },
  "devDependencies": {
    "@axe-core/react": "4.1.0",
    "@babel/core": "7.12.10",
    "@babel/runtime": "7.12.5",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@cypress/code-coverage": "3.9.2",
    "@next/bundle-analyzer": "10.0.5",
    "@next/eslint-plugin-next": "10.0.5",
    "@testing-library/react": "11.2.3",
    "@types/jest": "26.0.20",
    "@types/node": "13.13.36",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "babel-jest": "26.6.3",
    "commitizen": "4.2.3",
    "cypress": "6.3.0",
    "dependency-version-badge": "1.9.1",
    "eslint": "7.18.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jest-dom": "3.6.5",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.8",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.3",
    "next-compose-plugins": "2.2.1",
    "next-unused": "0.0.3",
    "npm-scripts-info": "0.3.9",
    "prettier-standard": "16.3.0",
    "standard-version": "9.1.0",
    "start-server-and-test": "1.11.7",
    "typedoc": "0.20.16",
    "typescript": "4.1.3"
  },
  "optionalDependencies": {
    "fsevents": "2.3.1"
  },
  "engines": {
    "node": ">=12",
    "pnpm": ">=5"
  },
  "pnpm": {
    "overrides": {
      "fsevents": "^2.3.1"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "pnpm type-check"
    }
  },
  "lint-staged": {
    "*": [
      "prettier-standard --lint"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "src",
      "styles",
      "test",
      "cypress"
    ],
    "exclude": [],
    "entrypoints": [
      "src"
    ]
  },
  "nyc": {
    "exclude": [
      "src/pages/api/__coverage__.js"
    ]
  }
}
