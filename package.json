{
  "name": "overnext",
  "version": "1.2.0",
  "description": "Very opinionated next.js typescript boilerplate",
  "keywords": [
    "next.js, cypress",
    "i18n",
    "typescript"
  ],
  "homepage": "https://overnext.vercel.app",
  "bugs": "https://github.com/einSelbst/overnext/issues",
  "author": "einSelbst",
  "private": true,
  "repository": "https://github.com/einSelbst/overnext",
  "scripts": {
    "prepare": "is-ci || husky install",
    "preinstall": "npx is-ci || npx only-allow pnpm",
    "postinstall": "is-ci || typesync",
    " ----------------------- Development ----------------------- ": "",
    "?dev": "echo Start next app in dev mode",
    "dev": "pnpm open && next",
    "?debug": "echo Start node debugger",
    "debug": "NODE_OPTIONS='--inspect' next",
    "?build": "echo Build the app",
    "build": "next build && pnpm build:sitemap",
    "build:sitemap": "next-sitemap",
    "?start": "echo Start server to serve the local app build",
    "start": "next start -p 3001",
    "?serve": "echo Build app and start server",
    "serve": "pnpm build && (open http://localhost:3002) && next start -p 3002",
    "serve:3003": "pnpm build && next start -p 3003",
    "?servex": "echo Build & run app on public URL",
    "servex": "pnpm serve & npx localtunnel --port 3000",
    " ------------------------- Testing ------------------------- ": "",
    "?test": "echo Run unit and integration tests once",
    "test": "jest && pnpm e2e",
    "?test:ci": "echo Run jest tests on CI, will re-add cypress later again",
    "test:ci": "jest",
    "?test:jest": "echo Run jest tests and update screenshots",
    "test:jest": "jest -u",
    "?test:cy": "echo Open cypress test runner (needs a running app server)",
    "test:cy": "cypress open",
    "?e2e:watch": "echo Start dev server, open cypress and watch",
    "e2e:watch": "CY_WATCH=true start-test 'next dev -p 3004' 3004 'pnpm test:cy'",
    "?e2e": "echo Build & serve app, then run cypress once",
    "e2e": "CYPRESS_BASE_URL=http://localhost:3003 APP_ENV=test start-test 'pnpm serve:3003' 3003 'cypress run'",
    "e2e:cy": "CYPRESS_BASE_URL=http://localhost:3001 APP_ENV=test start-test 'pnpm start' 3001 'cypress run'",
    "pree2e": "rm -rf .nyc_output coverage .next || true",
    " ------------------------- Linting ------------------------- ": "",
    "?format": "echo Format source code and fix issues",
    "format": "prettier-standard --format --lint",
    "?lint": "echo Lint source code and fix issues",
    "lint": "eslint . --fix",
    "?lint:html": "Run html-validate on generated static output files",
    "lint:html": "html-validate '.next/**/*.html'",
    "type-check": "tsc --pretty --noEmit",
    "?validate": "echo Validate source code and output files",
    "validate": "pnpm validate:src && pnpm validate:out",
    "?validate:src": "echo Format, lint, type-check & unit test the app",
    "validate:src": "pnpm format && pnpm lint && pnpm type-check && jest",
    "?validate:out": "echo Build app, run integration tests & validate generated html",
    "validate:out": "pnpm e2e && pnpm lint:html",
    " ------------------------- Helpers ------------------------- ": "",
    "?build:analyze": "echo Build app and output bundle analyzer diagrams",
    "build:analyze": "ANALYZE=true next build",
    "?build:size": "echo build app and measure loading performance",
    "build:size": "pnpm build && size-limit",
    "open": "open http://localhost:3000",
    "size": "size-limit",
    "?find:unused": "echo Find unused files",
    "find:unused": "next-unused",
    "?build:ssr": "echo used on CI for 'compressed size' action",
    "build:ssr": "NODE_ENV=production BROWSERSLIST_ENV=ssr next build",
    "?cz": "Make new commit",
    "cz": "git-cz",
    "?release": "Prepare a new release",
    "release": "standard-version",
    "?docs": "Generate typedoc docs",
    "docs": "typedoc",
    "?info": "echo Display info about the scripts",
    "info": "npm-scripts-info",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/core": "7.14.3",
    "@babel/runtime": "7.14.0",
    "@relative-ci/agent": "1.6.1",
    "@types/babel__core": "7.1.14",
    "@types/node": "14.17.1",
    "@types/prop-types": "15.7.3",
    "@types/react": "17.0.6",
    "@types/react-dom": "17.0.5",
    "@types/sass": "1.16.0",
    "autoprefixer": "10.2.5",
    "core-js": "3.12.1",
    "identity-obj-proxy": "3.0.0",
    "netlify-plugin-cypress": "2.2.0",
    "next": "10.2.3",
    "next-compose-plugins": "2.2.1",
    "next-pwa": "5.2.21",
    "next-seo": "4.24.0",
    "next-sitemap": "1.6.78",
    "next-themes": "0.0.14",
    "postcss": "8.3.0",
    "prop-types": "15.7.2",
    "querystring": "0.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "1.34.0",
    "start-server-and-test": "1.12.3",
    "tailwindcss": "2.1.2",
    "tslib": "2.2.0",
    "typescript": "4.2.4",
    "webpack": "5.37.1"
  },
  "devDependencies": {
    "@babel/preset-env": "7.14.2",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@cypress/code-coverage": "3.9.5",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@next/bundle-analyzer": "10.2.3",
    "@next/eslint-plugin-next": "10.2.3",
    "@size-limit/preset-app": "4.11.0",
    "@testing-library/react": "11.2.7",
    "@types/babel__preset-env": "7.9.1",
    "@types/core-js": "2.5.4",
    "@types/eslint": "7.2.11",
    "@types/is-ci": "3.0.0",
    "@types/jest": "26.0.23",
    "@types/source-map-support": "0.5.3",
    "@types/standard-version": "7.0.0",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "@typescript-eslint/typescript-estree": "4.25.0",
    "babel-jest": "26.6.3",
    "babel-plugin-istanbul": "6.0.0",
    "commitizen": "4.2.4",
    "cypress": "7.4.0",
    "cypress-watch-and-reload": "1.3.7",
    "dependency-version-badge": "1.10.0",
    "eslint": "7.27.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-compat": "3.9.0",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jest-dom": "3.9.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "eslint-plugin-unicorn": "32.0.1",
    "html-validate": "4.12.0",
    "husky": "6.0.0",
    "is-ci": "3.0.0",
    "jest": "26.6.3",
    "jest-watch-typeahead": "0.6.3",
    "lint-staged": "11.0.0",
    "netlify-plugin-a11y": "0.0.12",
    "next-unused": "0.0.6",
    "npm-scripts-info": "0.3.9",
    "nyc": "15.1.0",
    "prettier-standard": "16.3.0",
    "size-limit": "4.11.0",
    "source-map-support": "0.5.19",
    "standard-version": "9.3.0",
    "ts-node": "10.0.0",
    "typedoc": "0.20.36",
    "typescript-eslint-language-service": "4.1.4",
    "typesync": "0.8.0"
  },
  "optionalDependencies": {
    "@axe-core/react": "4.2.1",
    "fsevents": "2.3.2",
    "pnpm": "6.4.0"
  },
  "packageRules": [
    {
      "matchPackageNames": [
        "husky"
      ],
      "alowedVersions": "<=4"
    }
  ],
  "engines": {
    "node": ">=14",
    "pnpm": ">=5"
  },
  "pnpm": {
    "overrides": {
      "browserslist": "latest",
      "caniuse-lite": "latest",
      "chokidar": "3",
      "debug": "latest",
      "fsevents": "2.3.2",
      "prettierx": "0.18",
      "source-map-resolve": "latest"
    }
  },
  "lint-staged": {
    "*": [
      "prettier-standard --lint"
    ]
  },
  "next-unused": {
    "alias": {
      "lib/*": [
        "lib/*"
      ],
      "assets/*": [
        "assets/*"
      ],
      "config": [
        "config/"
      ],
      "components/*": [
        "components/*"
      ],
      "layouts/*": [
        "layouts/*"
      ],
      "styles/*": [
        "styles/*"
      ],
      "utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "src"
    ],
    "exclude": [],
    "entrypoints": [
      "src"
    ]
  },
  "volta": {
    "node": "14.17.0",
    "npm": "7.9.0"
  }
}
