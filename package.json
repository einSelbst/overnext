{
  "name": "feehikel",
  "author": "einSelbst",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:ssr": "NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "cz": "git-cz",
    "release": "standard-version",
    "docs": "typedoc",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier-standard --format --lint",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test:cy": "cypress open",
    "e2e": "start-test 'yarn dev' 3000 'yarn test:cy'",
    "pree2e": "rm -rf .nyc_output coverage .next || true",
    "validate": "yarn format && yarn lint && yarn type-check && yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*": [
      "prettier-standard --lint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "next": "latest",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@axe-core/react": "4.1.0",
    "@babel/core": "7.12.10",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@cypress/code-coverage": "3.9.0",
    "@next/bundle-analyzer": "10.0.5",
    "@next/eslint-plugin-next": "10.0.5",
    "@testing-library/react": "11.2.2",
    "@types/jest": "26.0.20",
    "@types/node": "13.13.36",
    "@types/react": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "babel-jest": "26.6.3",
    "commitizen": "4.2.2",
    "cypress": "6.2.1",
    "dependency-version-badge": "1.9.1",
    "eslint": "7.17.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jest-dom": "3.6.5",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.3",
    "next-compose-plugins": "2.2.1",
    "prettier-standard": "16.4.1",
    "standard-version": "9.1.0",
    "start-server-and-test": "1.11.7",
    "typedoc": "0.20.13",
    "typescript": "4.1.3"
  },
  "nyc": {
    "exclude": [
      "src/pages/api/__coverage__.js"
    ]
  }
}
